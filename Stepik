# s = input()
# counter = 0
# for i in range(len(s)):
#     counter += ord(s[i])
# print(f"–¢–µ–∫—Å—Ç —Å–æ–æ–±—â–µ–Ω–∏—è: '{s}'", f'–°—Ç–æ–∏–º–æ—Å—Ç—å —Å–æ–æ–±—â–µ–Ω–∏—è: {counter*3}üêù', sep='\n')

s = str(input())
counter = 0
for i in range(len(s)):
    counter += ord(s[i])
eng_letters = ['e', 'y', 'o', 'p', 'a', 'x', 'c', 'E', 'T', 'O', 'P', 'A', 'H', 'X', 'C', 'B', 'M']
ru_letters = ['–µ', '—É', '–æ', '—Ä', '–∞', '—Ö', '—Å', '–ï', '–¢', '–û', '–†', '–ê', '–ù', '–•', '–°', '–í', '–ú']
for g in range(len(s)):
    if g in eng_letters:
        g.replace(eng_letters, ru_letters)
counter1 = 0
for c in range(len(s)):
    counter1 += ord(s[c])
print(counter*3, counter1*3)

